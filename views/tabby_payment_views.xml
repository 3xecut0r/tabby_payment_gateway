<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_tabby_payment_list" model="ir.ui.view">
        <field name="name">tabby.payment.list</field>
        <field name="model">tabby.payment</field>
        <field name="arch" type="xml">
            <tree string="Tabby Payments">
                <field name="name"/>
                <field name="customer_name"/>
                <field name="customer_phone"/>
                <field name="customer_email"/>
                <field name="amount"/>
                <field name="payment_date"/>
                <field name="company_id"/>
            </tree>
        </field>
    </record>
</odoo>
